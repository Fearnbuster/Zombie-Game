        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Player Movement and Collision


//Setting up Variables
KeyRight = keyboard_check(ord("D"));
KeyLeft = -keyboard_check(ord("A"));
KeyJump = keyboard_check(vk_space);

//Movement
Move = KeyLeft + KeyRight;
Hsp = Move * MoveSpeed

if (CanMove = true)
    {
    //Gravity
    if (Vsp &lt; 10)
    {
    Vsp += Grav;
    }

    //Jumping
    if (place_meeting(x,y+1,obj_ground_parent))
        {
        Vsp = KeyJump * -JumpSpeed;
        }

    //Horizontal Collision
    if (place_meeting(x + Hsp,y,obj_ground_parent))
        {
        while (!place_meeting(x + sign(Hsp),y,obj_ground_parent))
            {
            x += sign(Hsp);
            }
        Hsp = 0;
        }

    //Vertical Collision
    if (place_meeting(x,y + Vsp,obj_ground_parent))
        {
        while (!place_meeting(x,y + sign(Vsp),obj_ground_parent))
            {
            y += sign(Vsp);
            }
        Vsp = 0;
        }

    x += Hsp;
    y += Vsp;
    }
</string>
