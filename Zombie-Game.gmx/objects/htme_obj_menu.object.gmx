            <string>//Creating Server
if keyboard_check_pressed(ord('C'))
{
    var Port = real(get_string("Port: ","6510"));

    if (Port != "")
    {
        //Starting Server
        if (htme_serverStart(Port,32))
        {
            room_goto(rm_dark_forest_area_1)
        }
        else
        {
            show_message("Failed to Start Server");
            game_end();
        }
    }
}

//Join server
if keyboard_check_pressed(ord('J'))
{
    var IP  = get_string("IP of Serer: ","127.0.0.1");
    var Port = real(get_string("Port: ","6510"));
    
    if (IP != "" &amp;&amp; Port &gt; 0)
    {
        if (htme_clientStart(IP,Port))
        {
            room_goto(htme_rm_connecting);
        }
        else
        {
            show_message("Failed to start Client");
        }
    }
}
</string>
